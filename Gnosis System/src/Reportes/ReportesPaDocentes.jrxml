	</parameter>
	<queryString>
		<![CDATA[SELECT
     tbDocentes."iddocente" AS tbDocentes_iddocente,
     tbDocentes."apellidos_docente" AS tbDocentes_apellidos_docente,
     tbDocentes."nombres_docente" AS tbDocentes_nombres_docente,
     tbDocentes."direccion" AS tbDocentes_direccion,
     tbDocentes."dui" AS tbDocentes_dui,
     tbDocentes."correo" AS tbDocentes_correo,
     tbDocentes."fecha_nac" AS tbDocentes_fecha_nac,
     tbDocentes."idgrado" AS tbDocentes_idgrado,
     tbDocentes."idgenero" AS tbDocentes_idgenero,
     tbDocentes."contacto" AS tbDocentes_contacto,
     tbGeneros."idgenero" AS tbGeneros_idgenero,
     tbGeneros."genero" AS tbGeneros_genero,
     tbGrados."idgrado" AS tbGrados_idgrado,
     tbGrados."grado" AS tbGrados_grado,
     tbGrados."idseccionAca" AS tbGrados_idseccionAca,
     tbGrados."idespecialidad" AS tbGrados_idespecialidad,
     tbGrados."idgrupo" AS tbGrados_idgrupo,
     tbGrados."idseccionTec" AS tbGrados_idseccionTec
FROM
     "dbo"."tbGeneros" tbGeneros INNER JOIN "dbo"."tbDocentes" tbDocentes ON tbGeneros."idgenero" = tbDocentes."idgenero"
     INNER JOIN "dbo"."tbGrados" tbGrados ON tbDocentes."idgrado" = tbGrados."idgrado"
WHERE tbDocentes.dui=$P!{DUI}]]>
	</queryString>
